    "@opam/caqti-lwt": "1.2.0",
    "@opam/cohttp": "2.5.4",
    "@opam/cohttp-lwt-unix": "2.5.4",
    "@opam/dune": "2.7.1",
    "@opam/fpath": "0.7.3",
    "@opam/lwt": "5.3.0",
    "@opam/lwt_ppx": "2.0.1",
    "@opam/lwt_ssl": "1.1.3",
